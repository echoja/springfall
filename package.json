{
  "name": "monnomlog",
  "version": "0.1.0",
  "author": "EzKorry",
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn db:studio\"",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "build": "yarn db:push && next build && next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "upgrade": "yarn upgrade-interactive",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "make-hook-executable": "chmod ug+x .husky/* && chmod ug+x .git/hooks/*"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.6",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/pro-regular-svg-icons": "^6.1.1",
    "@fortawesome/pro-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.11.1",
    "framer-motion": "^6.2.8",
    "ky": "^0.30.0",
    "markdown-it": "^12.3.2",
    "next": "^12.1.1",
    "next-auth": "^4.3.1",
    "next-seo": "^5.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-markdown-editor-lite": "^1.3.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.0.0",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^17.0.43",
    "commitlint": "^16.2.3",
    "concurrently": "^7.0.0",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sznm": "^0.2.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "next-sitemap": "^2.5.14",
    "prettier": "^2.6.1",
    "prisma": "^3.11.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.6.3"
  }
}
