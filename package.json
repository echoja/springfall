{
  "name": "monnomlog",
  "version": "0.1.0",
  "author": "EzKorry",
  "scripts": {
    "t": "vitest",
    "test": "vitest",
    "test:ci": "vitest run",
    "dev": "next dev",
    "build": "next build && next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "analyze": "ANALYZE=true yarn build",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "gen:sb": "cat src/modules/supabase/supabase-types-beginning.txt > src/modules/supabase/supabase-types.ts && supabase gen types typescript --local >> src/modules/supabase/supabase-types.ts && prettier --write src/modules/supabase/supabase-types.ts",
    "gen": "pnpm gen:sb",
    "sb-diff": "supabase db diff --use-migra -f",
    "sb-push": "supabase db push",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "make-hook-executable": "chmod ug+x .husky/* && chmod ug+x .git/hooks/*"
  },
  "dependencies": {
    "@floating-ui/react-dom-interactions": "^0.10.3",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/pro-regular-svg-icons": "^6.2.1",
    "@fortawesome/pro-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.5",
    "@next/bundle-analyzer": "^13.0.6",
    "@next/env": "^13.0.6",
    "@supabase/supabase-js": "2.1.3",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^1.2.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "framer-motion": "^7.6.19",
    "hotkeys-js": "^3.10.1",
    "immer": "^9.0.16",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "joi": "^17.7.0",
    "jotai": "^1.11.1",
    "markdown-it": "^13.0.1",
    "nanoid": "^4.0.0",
    "next": "^13.0.6",
    "next-seo": "^5.15.0",
    "nextjs-google-analytics": "^2.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-merge-refs": "^2.0.1",
    "refractor": "^4.8.0",
    "rfdc": "^1.3.0",
    "slate": "^0.87.0",
    "slate-history": "^0.86.0",
    "slate-hyperscript": "^0.77.0",
    "slate-react": "^0.87.0",
    "string-strip-html": "^13.0.2",
    "tailwind-merge": "^1.8.0",
    "usehooks-ts": "^2.9.1",
    "vite": "^4.0.0",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "@types/is-hotkey": "^0.1.7",
    "@types/is-url": "^1.2.30",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^18.0.26",
    "autoprefixer": "^10.4.13",
    "commitlint": "^17.3.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sznm": "^1.0.1",
    "happy-dom": "^8.1.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "next-sitemap": "^3.1.32",
    "postcss": "^8.4.19",
    "prettier": "^2.8.1",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.2.4",
    "type-fest": "^3.3.0",
    "typescript": "^4.9.4",
    "vitest": "^0.25.7"
  }
}
