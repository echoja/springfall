{
  "name": "monnomlog",
  "version": "0.1.0",
  "author": "EzKorry",
  "scripts": {
    "t": "vitest",
    "test": "vitest",
    "test:ci": "vitest run",
    "dev": "next dev",
    "build": "next build && next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "analyze": "ANALYZE=true yarn build",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "gen:sb": "cat src/modules/supabase/supabase-types-beginning.txt > src/modules/supabase/supabase-types.ts && supabase gen types typescript --local >> src/modules/supabase/supabase-types.ts && prettier --write src/modules/supabase/supabase-types.ts",
    "gen": "pnpm gen:sb",
    "sb-diff": "supabase db diff --use-migra -f",
    "sb-push": "supabase db push",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "make-hook-executable": "chmod ug+x .husky/* && chmod ug+x .git/hooks/*"
  },
  "dependencies": {
    "@floating-ui/react": "^0.21.0",
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@next/bundle-analyzer": "^13.2.4",
    "@next/env": "^13.2.4",
    "@supabase/supabase-js": "2.10.0",
    "@tailwindcss/forms": "^0.5.3",
    "@upstash/qstash": "^0.3.6",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "encoding": "^0.1.13",
    "fast-xml-parser": "^4.1.3",
    "framer-motion": "^9.1.7",
    "hotkeys-js": "^3.10.1",
    "html-tags": "^3.2.0",
    "immer": "^9.0.19",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "joi": "^17.8.3",
    "jotai": "^2.0.3",
    "markdown-it": "^13.0.1",
    "nanoid": "^4.0.1",
    "next": "^13.2.4",
    "next-seo": "^5.15.0",
    "nextjs-google-analytics": "^2.3.3",
    "parse5": "^7.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-merge-refs": "^2.0.1",
    "refractor": "^4.8.1",
    "rfdc": "^1.3.0",
    "slate": "^0.91.4",
    "slate-history": "^0.86.0",
    "slate-hyperscript": "^0.77.0",
    "slate-react": "^0.91.10",
    "string-strip-html": "^13.2.1",
    "tailwind-merge": "^1.10.0",
    "usehooks-ts": "^2.9.1",
    "vite": "^4.1.4",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@types/is-hotkey": "^0.1.7",
    "@types/is-url": "^1.2.30",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^18.0.28",
    "autoprefixer": "^10.4.14",
    "commitlint": "^17.4.4",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-sznm": "^1.1.0",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "next-sitemap": "^3.1.55",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.2.7",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  }
}
