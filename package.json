{
  "name": "nextarter-chakra",
  "version": "0.1.1",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn db-studio\"",
    "db-studio": "prisma studio",
    "db-push": "prisma db push",
    "build": "next-sitemap --config next-sitemap.config.js && yarn db-push && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "upgrade": "yarn upgrade-interactive",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "make-hook-executable": "chmod ug+x .husky/* && chmod ug+x .git/hooks/*"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.1",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@fontsource/lexend": "^4.5.3",
    "@next-auth/prisma-adapter": "^1.0.1",
    "@prisma/client": "^3.9.1",
    "framer-motion": "^5.6.0",
    "next": "^12.0.9",
    "next-auth": "^4.1.2",
    "next-pwa": "^5.4.4",
    "next-seo": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.0.0",
    "@types/react": "^17.0.38",
    "commitlint": "^16.1.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.9",
    "eslint-config-sznm": "^0.2.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "next-sitemap": "^2.1.5",
    "prettier": "^2.5.1",
    "prisma": "^3.9.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.5"
  }
}
