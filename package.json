{
  "name": "springfall",
  "version": "0.1.0",
  "author": "EzKorry",
  "scripts": {
    "t": "vitest",
    "test": "vitest",
    "test:ci": "vitest run",
    "dev": "next dev",
    "build": "next build && next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "analyze": "ANALYZE=true yarn build",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "gen:sb": "supabase status && cat src/modules/supabase/supabase-types-beginning.txt > src/modules/supabase/supabase-types.ts && supabase gen types typescript --local >> src/modules/supabase/supabase-types.ts && prettier --write src/modules/supabase/supabase-types.ts",
    "gen": "pnpm gen:sb",
    "sb-diff": "supabase db diff --use-migra -f",
    "sb-push": "supabase db push",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "make-hook-executable": "chmod ug+x .husky/* && chmod ug+x .git/hooks/*"
  },
  "dependencies": {
    "@code-hike/mdx": "^0.9.0",
    "@echoja/remark-custom-container": "^1.3.2",
    "@floating-ui/react": "^0.24.8",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/bundle-analyzer": "^13.4.10",
    "@next/env": "^13.4.10",
    "@next/mdx": "^13.4.10",
    "@supabase/supabase-js": "2.26.0",
    "@tailwindcss/forms": "^0.5.4",
    "@upstash/qstash": "^0.3.6",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dayjs": "^1.11.9",
    "encoding": "^0.1.13",
    "fast-xml-parser": "^4.2.5",
    "framer-motion": "^10.12.21",
    "hotkeys-js": "^3.11.2",
    "html-tags": "^3.3.1",
    "immer": "^10.0.2",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "joi": "^17.9.2",
    "jotai": "^2.2.2",
    "markdown-it": "^13.0.1",
    "nanoid": "^4.0.2",
    "next": "^13.4.10",
    "next-seo": "^6.1.0",
    "nextjs-google-analytics": "^2.3.3",
    "parse5": "^7.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-merge-refs": "^2.0.2",
    "refractor": "^4.8.1",
    "remark-gfm": "^3.0.1",
    "rfdc": "^1.3.0",
    "sharp": "^0.32.3",
    "slate": "^0.94.1",
    "slate-history": "^0.93.0",
    "slate-hyperscript": "^0.77.0",
    "slate-react": "^0.97.2",
    "string-strip-html": "^13.4.2",
    "tailwind-merge": "^1.13.2",
    "usehooks-ts": "^2.9.1",
    "vite": "^4.4.4",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.6",
    "@types/is-hotkey": "^0.1.7",
    "@types/is-url": "^1.2.30",
    "@types/markdown-it": "^12.2.3",
    "@types/mdx": "^2.0.5",
    "@types/react": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "commitlint": "^17.6.6",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "happy-dom": "^10.3.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "next-sitemap": "^4.1.8",
    "postcss": "^8.4.26",
    "prettier": "^3.0.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.3.3",
    "type-fest": "^3.13.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  }
}
