{
  "name": "monnomlog",
  "version": "0.1.0",
  "author": "EzKorry",
  "scripts": {
    "t": "vitest",
    "test": "vitest",
    "dev": "next dev",
    "build": "next build && next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "analyze": "ANALYZE=true yarn build",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
