{
  "name": "monnomlog",
  "version": "0.1.0",
  "author": "EzKorry",
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn db:studio\"",
    "db:studio": "prisma studio",
    "db:push": "prisma db push --accept-data-loss",
    "db:generate": "prisma generate",
    "build": "yarn db:push && next build && next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "upgrade": "yarn upgrade-interactive",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "make-hook-executable": "chmod ug+x .husky/* && chmod ug+x .git/hooks/*"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^0.6.3",
    "@floating-ui/react-dom-interactions": "^0.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/pro-regular-svg-icons": "^6.1.1",
    "@fortawesome/pro-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@headlessui/react": "^1.6.1",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.13.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@tailwindcss/forms": "^0.5.1",
    "cuid": "^2.1.8",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.3.3",
    "hotkeys-js": "^3.9.3",
    "joi": "^17.6.0",
    "ky": "^0.30.0",
    "markdown-it": "^13.0.0",
    "next": "^12.1.5",
    "next-auth": "^4.3.3",
    "next-seo": "^5.4.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.3.1",
    "react-markdown-editor-lite": "^1.3.2",
    "react-merge-refs": "^1.1.0",
    "rfdc": "^1.3.0",
    "slate": "^0.78.0",
    "slate-react": "^0.78.1",
    "tailwind-merge": "^1.2.1",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.0.0",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^18.0.8",
    "autoprefixer": "^10.4.5",
    "commitlint": "^16.2.3",
    "concurrently": "^7.0.0",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sznm": "^0.2.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "next-sitemap": "^2.5.20",
    "postcss": "^8.4.13",
    "prettier": "^2.6.1",
    "prisma": "latest",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.24",
    "type-fest": "^2.12.2",
    "typescript": "^4.6.3"
  }
}
