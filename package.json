{
  "name": "monnomlog",
  "version": "0.1.0",
  "author": "EzKorry",
  "scripts": {
    "t": "jest",
    "dev": "next dev",
    "build": "next build && next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "codegen": "cat src/modules/supabase/supabase-types-beginning.txt > src/modules/supabase/supabase-types.ts && openapi-typescript \"${NEXT_PUBLIC_SUPABASE_URL}/rest/v1/?apikey=${NEXT_PUBLIC_SUPABASE_ANON_KEY}\" >> src/modules/supabase/supabase-types.ts",
    "upgrade": "yarn upgrade-interactive",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "make-hook-executable": "chmod ug+x .husky/* && chmod ug+x .git/hooks/*"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/pro-regular-svg-icons": "^6.1.1",
    "@fortawesome/pro-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.6.6",
    "@supabase/supabase-js": "^1.35.4",
    "@tailwindcss/forms": "^0.5.2",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.5.1",
    "hotkeys-js": "^3.9.4",
    "immer": "^9.0.15",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "joi": "^17.6.0",
    "jotai": "^1.7.6",
    "markdown-it": "^13.0.1",
    "nanoid": "^4.0.0",
    "next": "^12.2.2",
    "next-seo": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-merge-refs": "^2.0.1",
    "refractor": "^4.7.0",
    "rfdc": "^1.3.0",
    "slate": "^0.81.1",
    "slate-history": "^0.66.0",
    "slate-react": "^0.81.0",
    "string-strip-html": "^10.1.0",
    "tailwind-merge": "^1.4.0",
    "usehooks-ts": "^2.6.0",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.3",
    "@swc-node/jest": "^1.5.2",
    "@types/is-url": "^1.2.30",
    "@types/jest": "^28.1.6",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^18.0.15",
    "autoprefixer": "^10.4.7",
    "commitlint": "^17.0.3",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-config-next": "^12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sznm": "^1.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "next-sitemap": "^3.1.11",
    "openapi-typescript": "^5.4.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.1.6",
    "type-fest": "^2.16.0",
    "typescript": "^4.7.4"
  }
}
